{
  "name": "farmers-ordering-api",
  "version": "1.0.0",
  "description": "A farmers ordering system",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "npm test",
    "sequelize": "ts-node -r tsconfig-paths/register node_modules/.bin/sequelize-cli",
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:seed": "node_modules/.bin/sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-braham/farmers-ordering-api.git"
  },
  "keywords": [
    "farmers",
    "order",
    "farmers-ordering"
  ],
  "author": "kirumba.kamau@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a-braham/farmers-ordering-api/issues"
  },
  "homepage": "https://github.com/a-braham/farmers-ordering-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.13",
    "@types/sequelize": "^4.28.20",
    "express": "^4.19.2",
    "nodemon": "^3.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "celebrate": "^15.0.3",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "joi": "^17.13.1",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2"
  }
}
